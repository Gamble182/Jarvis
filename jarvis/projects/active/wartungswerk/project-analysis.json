  "risks": [
    {
      "id": "RISK-001",
      "category": "technical",
      "description": "Email-Zustellung - Emails landen im Spam",
      "impact": "high",
      "mitigation": "Resend nutzen, SPF/DKIM/DMARC konfigurieren, Opt-In dokumentieren"
    },
    {
      "id": "RISK-002",
      "category": "technical",
      "description": "Offline-Sync-Konflikte",
      "impact": "medium",
      "mitigation": "Last-Write-Wins Strategie, Konflikt-Flag bei Sync"
    },
    {
      "id": "RISK-010",
      "category": "business",
      "description": "Max nutzt System nicht aktiv",
      "impact": "very-high",
      "mitigation": "Intensive 2h Schulung, wöchentliche Check-Ins, kontinuierlicher Feedback-Loop"
    },
    {
      "id": "RISK-020",
      "category": "legal",
      "description": "DSGVO-Verstoß",
      "impact": "very-high",
      "mitigation": "Rechtsanwalt konsultieren, Double-Opt-In, AV-Vertrag mit Supabase"
